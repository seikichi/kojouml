{
  "name": "kojouml",
  "description": "PlantUML javascript clone",
  "version": "0.0.1",
  "author": "Seikichi KONDO",
  "bugs": {
    "url": "https://github.com/seikichi/kojouml/issues"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.69",
    "@types/webpack": "^2.2.7",
    "@types/webpack-merge": "^0.0.4",
    "awesome-typescript-loader": "^3.0.8",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.0",
    "prettier": "^1.4.4",
    "ts-node": "^3.1.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.4.1",
    "viz.js": "^1.7.0",
    "webpack": "^2.2.1",
    "webpack-merge": "^3.0.0"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/seikichi/kojouml#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "pegjs",
      "json"
    ],
    "transform": {
      "\\.(ts|tsx|pegjs)$": "<rootDir>/jest.preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/src/**/*\\.test\\.(ts|tsx|js)"
    ]
  },
  "keywords": [
    "PlanUML",
    "UML"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seikichi/kojouml.git"
  },
  "scripts": {
    "build": "webpack",
    "format": "prettier --single-quote --trailing-comma all --write 'src/**/*.ts'",
    "test": "jest"
  }
}
